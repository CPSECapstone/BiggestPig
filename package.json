{
  "name": "cloudhaven",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.x"
  },
  "main": "components/index.js",
  "dependencies": {
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "axios": "^0.21.0",
    "concurrently": "latest",
    "nodemon": "latest",
    "pg-promise": "^10.7.3",
    "react": "latest",
    "react-dom": "latest",
    "react-router-dom": "latest",
    "react-scripts": "latest"
  },
  "type": "module",
  "description": "https://github.com/mui-org/material-ui/blob/master/docs/src/pages/components/text-fields/FormPropsTextFields.js",
  "scripts": {
    "update": "npm install",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "client": "yarn start",
    "server": "nodemon api/server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "react-scripts start",
    "heroku-postbuild": "npm install --only=dev --no-shrinkwrap && react-scripts build"
  },
  "proxy": "http://localhost:5000/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
