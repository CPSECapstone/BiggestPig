{
  "name": "cloudhaven",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "main": "cloudhaven/client/src/index.js",
  "dependencies": {
    "react-dom": "latest",
    "react": "latest",
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "concurrently": "latest"
  },
  "devDependencies": {
    "@material-ui/icons": "^4.9.1",
    "react-scripts": "latest"
  },
  "description": "https://github.com/mui-org/material-ui/blob/master/docs/src/pages/components/text-fields/FormPropsTextFields.js",
  "scripts": {
    "update": "npm install && cd client && npm install",
    "build": "react-scripts build",
    "test": "cd client && react-scripts test",
    "client": "cd client && yarn start",
    "server": "nodemon server/server.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "start": "node cloudhaven/client/src/index.js",
    "heroku-postbuild": "cd client && npm install && npm install --only=dev --no-shrinkwrap && npm run build"
  },
  "proxy": "http://localhost:5000/",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
